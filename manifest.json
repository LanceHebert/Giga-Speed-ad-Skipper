{
    "manifest_version": 3,

    "name": "Giga-Speed Youtube ad skipper",
    "description": "15x speed enhancer to fast forward through youtube in video player ads. Otherwise this functions to toggle between 1x,2x,3x video playback speed controls",
    "version": "1.0",
    
    "content_scripts": [
        {
            "matches": [
              "https://*.youtube.com/*"
            ],
            "js": [
                "src/javascript/script.js"
            ],
            "css": [
                "src/css/styles.css"
            ]
        }
    ],

    "icons": {
        "16": "src/images/icon16.png",
        "48": "src/images/icon48.png",
        "128": "src/images/icon128.png"
    }
}